#include "kernel.h"
#include "swis.h"

#include "unixlib.h"
#include "stubs.h"

long getpid(void)
{
    _kernel_swi_regs regs;

    regs.r[0] = 5;
    if (_kernel_swi(Wimp_ReadSysInfo, &regs, &regs) != NULL || regs.r[0] == 0)
        return(UNIX_MagicNumber_GID_Nobody);

    return regs.r[0];
}

/* EOF getpid.c */
