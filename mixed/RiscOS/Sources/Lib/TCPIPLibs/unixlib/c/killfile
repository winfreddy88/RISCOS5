#include "kernel.h"
#include "swis.h"

#include "unixlib.h"

void killfile(const char *name)
{
    (void)_swix(OS_File, _INR(0,1), 6, name);
}

/*
 * this is the proper UNIX function name
 */
int unlink(const char *path)
{
    killfile(path);

    return(0);
}

/* EOF killfile.c */
