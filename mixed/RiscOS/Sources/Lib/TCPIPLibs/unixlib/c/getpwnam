#include <string.h>
#include "pwd.h"
#include "stubs.h"

#include "unixlib.h"

struct passwd *getpwnam(const char *name)
{
    memset((char *)&_pwbuf, 0, sizeof(_pwbuf));

    _pwbuf.pw_name = (char *)name;
    _pwbuf.pw_uid = UNIX_MagicNumber_UID_Nobody;
    _pwbuf.pw_gid = UNIX_MagicNumber_GID_Nobody;

    return (&_pwbuf);
}

/* EOF getpwnam.c */
