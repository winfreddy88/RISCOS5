        [       :LNOT: :DEF: __HAL_UART_HDR__
        GBLL    __HAL_UART_HDR__

        GET     Hdr.iMx6qMemMap
        GET     Hdr.iMx6qReg
        GET     Hdr.iMx6qIRQs


UARTCLK         *       80000000 ; default UART_CLK_PODF o/p 80 MHz

UART_FIFODepth  *       32       ; depth of uart fifos

