#
# BBE Makefile fragment for rom_link for OHCIDriver
#

rom_link-OHCIDriver:
        ${MKDIR} linked
        ${LD} -o linked.${TARGET} -rmf -base ${ADDRESS} aof.${TARGET} RISC_OSLib:o.abssym
        ${CP} linked.${TARGET} ${LINKDIR}.${TARGET} ${CPFLAGS}
        @${ECHO} ${COMPONENT}: rom_link complete

