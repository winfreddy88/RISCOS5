/* Title:   upcalls.c
 * Purpose: to handle any up-calls from txtedit
 * Author:  IDJ
 * History: 01-Mar-90: IDJ: created
 *
 */

#include "multiedit.h"
#include "winevent.h"
#include "throwback.h"

#include "upcalls.h"

extern void upcalls_open_handler(char *filename, txtedit_state *s, void *handle)
{
   /* call other open handlers (NB order is important!!!!) */
   winevent_open_handler(filename, s, handle);
   multiedit_open_handler(filename, s, handle);
}

extern void upcalls_close_handler(char *filename, txtedit_state *s, void *handle)
{
   /* call other close handlers */
   throwback_close_handler(filename, s, handle);
}



   




