/*
 * The global entry points for italkmsg.c
 */

#ifndef __italkmsg_global__
#define __italkmsg_global__

extern void italkmsg_init(void);

extern _kernel_oserror *send_Connect_command(void);
extern _kernel_oserror *send_CheckURLAccess_request(char *user, char *url, int *ref);
extern _kernel_oserror *send_GetURLExpire_request(char *url);
extern _kernel_oserror *send_GetURLCache_request(int set_default);
extern _kernel_oserror *send_ResetTimer_command(void);


#endif /* __italkmsg_global__ */
