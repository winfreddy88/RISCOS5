/*->c.args */


#include <stdio.h>
#include <stdlib.h>
#include <string.h>


#include "kernel.h"
#include "swis.h"


#include "etc.h"

#include "err.h"

#include "mem.h"
#include "rminfo.h"
#include "flex.h"


#include "constants.h"

#include "fs.h"
#include "buffer.h"
#include "zip.h"
#include "inflate.h"
#include "unzip.h"

#include "vectors.h"

#include "find.h"

#include "args.h"




int argspath(_kernel_swi_regs * r)
{
 int fh;
 fh=r->r[1];
 r->r[1]=findsfh2rfh(fh);
 return((r->r[1]==fh)?1:2);
}



int argspathp(_kernel_swi_regs * r)
{
 r->r[1]=findrfh2sfh(r->r[1]);
 return(1);
}


