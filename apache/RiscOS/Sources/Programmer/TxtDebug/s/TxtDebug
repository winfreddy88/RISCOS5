; 07-Jan-05  0.01     BJGA  Initial version, based on DADebug and MPEGDriver


; Compile-time options

        GBLL    Margin
Margin  SETL    {TRUE}


; Includes

        GET     Hdr:ListOpts
        GET     Hdr:Macros
        GET     Hdr:System
        GET     Hdr:Machine.<Machine>
        GET     Hdr:Proc
        GET     Hdr:ModHand
        GET     Hdr:VduExt
        GET     Hdr:Services
        GET     Hdr:ColourTran
        GET     VersionASM


; Module header

Module_BaseAddr
        &       0; Start
        &       Initialise - Module_BaseAddr
        &       Finalise - Module_BaseAddr
        &       Service - Module_BaseAddr
        &       Title - Module_BaseAddr
        &       Help - Module_BaseAddr
        &       0 ; CommandTable
        &       DADebugSWI_Base
        &       SWIHandler - Module_BaseAddr
        &       SWITable - Module_BaseAddr
        &       0 ; SWI decoding code
        [ {FALSE} ; International_Help <> 0
        &       MessagesFile - Module_BaseAddr
        |
        &       0
        ]
        &       ModuleFlags - Module_BaseAddr

Help
        =       "DADebug", 9, "$Module_MajorVersion ($Module_Date)"
        [ Module_MinorVersion <> ""
        =       " $Module_MinorVersion"
        ]
        =       0
Title
SWITable
        =       "DADebug", 0
        =       "GetWriteCAddress", 0
        =       0
        ALIGN

ModuleFlags
        [ No32bitCode
        &       0
        |
        &       ModuleFlag_32bit
        ]

Initialise
        Entry
        ADR     r0, WriteC
        STR     r0, TrampolineTemplateEnd-4 ; relocate
        BL      ReadVduVariables
        EXIT

Finalise
        Entry
        CLRV
        EXIT

ServiceTable
        DCD     0
        DCD     Service2 - Module_BaseAddr
        DCD     Service_ModeChange ; &46
        DCD     0
        DCD     ServiceTable - Module_BaseAddr
Service
        MOV     R0, R0
        TEQ     R1, #Service_ModeChange
        MOVNE   PC, R14
Service2
        ; drop through...

ReadVduVariables
        Entry   "r0-r5"
        SUB     sp, sp, #6*4
        ADR     r0, %FT90
        MOV     r1, sp
        SWI     XOS_ReadVduVariables
        LDMIA   sp!, {r0-r5}
        ADRL    lr, ncols
        MOV     r1, r1, LSR #1 ; double height => half as many lines
      [ Margin
        SUB     r0, r0, #5
        SUB     r1, r1, #2
        STMIA   lr, {r0-r3}
        MOV     r0, #5
        MOV     r1, #2
      |
        STMIA   lr, {r0-r3}
        MOV     r0, #0
        MOV     r1, #0
      ]
        ADRL    lr, hpos
        STMIA   lr, {r0-r1,r4-r5}
        ADR     lr, PlotCharTable
        LDR     r4, [lr, r5, LSL #2]
        ADD     r4, lr, r4
        STR     r4, PlotChar

        LDR     r4, clientshead
        TEQ     r4, #0
        EXIT    EQ
01      BL      CreateExpandTable
        LDR     r4, [r4, #Client_Next]
        TEQ     r4, #0
        BNE     %BT01
        EXIT
90
        &       VduExt_ScrRCol, VduExt_ScrBRow, VduExt_ScreenStart, VduExt_LineLength
        &       VduExt_Log2BPP, VduExt_Log2BPC, -1

PlotCharTable
        &       PlotChar1  - PlotCharTable
        &       PlotChar2  - PlotCharTable
        &       PlotChar4  - PlotCharTable
        &       PlotChar8  - PlotCharTable
        &       PlotChar16 - PlotCharTable
        &       PlotChar32 - PlotCharTable


SWIHandler ROUT
        CMP     R11, #(EndOfJumpTable - JumpTable)/4
        ADDLO   PC, PC, R11, LSL #2
        B       UnknownSWIError
JumpTable
        B       SWI_GetWriteCAddress
EndOfJumpTable

UnknownSWIError
        MOV     pc, lr           ; don't actually return an error for now


TrampolineTemplate
        Push    "r4,lr"
        ADR     r4, TrampolineTemplateEnd ; -> text expand table
        MOV     lr, pc
        LDR     pc, [pc]
        Pull    "r4,pc"
        &       WriteC
TrampolineTemplateEnd

                ^       0
Client_Next     #       4
Client_FG       #       4
Client_BG       #       4
Client_Tramp    #       TrampolineTemplateEnd - TrampolineTemplate
Client_Expand   #       4*8*256
ClientSize      *       :INDEX: @


; In: R0 = flags (reserved, sbz)
;     R1 = magic word (else behave as though R0=0, R2=&FFFFFF00, R3=&00000AA)
;     R2 = foreground colour, &BBGGRRTT
;     R3 = background colour, &BBGGRRTT
SWI_GetWriteCAddress
        Entry    "r1-r7"
        LDR     lr, =&C0102DEB
        TEQ     r1, lr
        MOVNE   r2, #&FFFFFF00
        MOVNE   r3, #&000000AA

        LDR     r4, clientshead
        TEQ     r4, #0
        BEQ     %FT20
        ASSERT  Client_FG = Client_Next + 4
        ASSERT  Client_BG = Client_Next + 8
01      LDMIA   r4, {r5-r7}
        TEQ     r2, r6
        TEQEQ   r3, r7
        ADDEQ   r0, r4, #Client_Tramp
        EXIT    EQ ; return pointer to duplicate of existing client
        MOVS    r4, r5
        BNE     %BT01
20      ; create new client struct
        Push    "r0,r2,r3"
        MOV     r0, #ModHandReason_Claim
        LDR     r3, =ClientSize
        SWI     XOS_Module
        ADDVS   sp, sp, #4*3
        EXIT    VS
        MOV     r4, r2
        Pull    "r0,r2,r3"
        LDR     r1, clientshead
        STMIA   r4!, {r1-r3}
        ASSERT  TrampolineTemplateEnd - TrampolineTemplate = 6*4
        ADR     lr, TrampolineTemplate
        LDMIA   lr, {r1-r3,r5-r7}
        STMIA   r4!, {r1-r3,r5-r7}
        Push    "r0"
        MOV     r0, #0
        SWI     XOS_SynchroniseCodeAreas ; too lazy to specify range
        Pull    "r0"
        SUB     r4, r4, #Client_Expand
        STR     r4, clientshead
        BL      CreateExpandTable
        ADD     r0, r4, #Client_Tramp
        EXIT

; In: R4 -> client block
; Out: R4 preserved, V clear
CreateExpandTable
        Entry   "r0-r5"
        LDR     r1, log2bpp

        LDR     r2, [r4, #Client_FG]
        BIC     r0, r2, #&FF
        SWI     XColourTrans_ReturnColourNumber
        RSBS    lr, r1, #4 ; 16bpp
        BICCC   r0, r0, #&FF000000
        ORRCC   r0, r0, r2, LSL #24 ; put transparency bits back in 32bpp
        ORREQ   r0, r0, #&8000
        TSTEQ   r2, #&80
        BICEQ   r0, r0, #&8000 ; use msb of transparency in 16bpp
        ORRCS   r0, r0, r0, LSL #16
        CMP     r1, #3 ; 8bpp
        ORRLS   r0, r0, r0, LSL #8
        ORRLO   r0, r0, r0, LSL #4
        CMP     r1, #1 ; 2bpp
        ORRLS   r0, r0, r0, LSL #2
        ORRLO   r0, r0, r0, LSL #1
        MOV     r2, r0 ; completed fg colour number replicated across a word

        LDR     r3, [r4, #Client_BG]
        BIC     r0, r3, #&FF
        SWI     XColourTrans_ReturnColourNumber
        RSBS    lr, r1, #4 ; 16bpp
        BICCC   r0, r0, #&FF000000
        ORRCC   r0, r0, r3, LSL #24 ; put transparency bits back in 32bpp
        ORREQ   r0, r0, #&8000
        TSTEQ   r3, #&80
        BICEQ   r0, r0, #&8000 ; use msb of transparency in 16bpp
        ORRCS   r0, r0, r0, LSL #16
        CMP     r1, #3 ; 8bpp
        ORRLS   r0, r0, r0, LSL #8
        ORRLO   r0, r0, r0, LSL #4
        CMP     r1, #1 ; 2bpp
        ORRLS   r0, r0, r0, LSL #2
        ORRLO   r0, r0, r0, LSL #1
        MOV     r3, r0 ; completed bg colour number replicated across a word

        LDR     r0, log2bpc
        ADR     r5, WidthExpandTables
        LDR     lr, [r5, r0, LSL #2] ; r5 -> appropriate table
        ADD     r5, r5, lr
        MOV     lr, #256
        MOV     lr, lr, LSL r0 ; lr = size of table
        ADD     r4, r4, #Client_Expand
01      LDR     r0, [r5], #4
        BIC     r1, r3, r0
        AND     r0, r2, r0
        ORR     r0, r0, r1
        STR     r0, [r4], #4
        SUBS    lr, lr, #4
        BNE     %BT01
        EXIT

WidthExpandTables
        &       WidthExpandTable1  - WidthExpandTables
        &       WidthExpandTable2  - WidthExpandTables
        &       WidthExpandTable4  - WidthExpandTables
        &       WidthExpandTable8  - WidthExpandTables
        &       WidthExpandTable16 - WidthExpandTables
        &       WidthExpandTable32 - WidthExpandTables


WriteC
; In: R0 = character to insert
;     R4 -> text expand table
;     R14 = return address
;     Assumed not in USR mode
; Out: all registers and flags must be preserved
        EntryS  "r0-r3"
        AND     r0, r0, #&FF
        TEQ     r0, #10
        BEQ     %FT10
        TEQ     r0, #13
        BEQ     %FT13
        TEQ     r0, #127
        BEQ     %FT99
        CMP     r0, #32
        EXITS   CC

        SETPSR  I_bit, R14 ; execute with IRQs off

        ADRL    lr, screenbase
        ASSERT  linelength = screenbase + 4
        ASSERT  hpos = screenbase + 8
        ASSERT  vpos = screenbase + 12
        LDMIA   lr, {r0-r3}
        LDR     lr, [lr, #log2bpc - screenbase]
        MOV     lr, r2, LSL lr
        MUL     r3, r1, r3
        ADD     lr, lr, r3, LSL #4

        LDR     r3, ncols
        ADD     r2, r2, #1
        CMP     r2, r3
      [ Margin
        MOVEQ   r2, #5
      |
        MOVEQ   r2, #0
      ]
        STR     r2, hpos

        LDREQ   r2, vpos
        ADDEQ   r2, r2, #1
        STREQ   r2, vpos
        LDREQ   r3, nrows
        CMPEQ   r2, r3
      [ Margin
        MOVEQ   r2, #2
      |
        MOVEQ   r2, #0
      ]
        STREQ   r2, vpos

        ADD     r2, r0, lr ; screenbase + hpos<<log2bpc + 16*linelength*vpos
        LDR     r0, [sp, #Proc_RegOffset]
        MOV     lr, pc
        LDR     pc, PlotChar
        EXITS   , cxsf
10      ; LF
        LDR     r0, vpos
        ADD     r0, r0, #1
        STR     r0, vpos
        LDR     r1, nrows
        CMP     r0, r1
      [ Margin
        MOVEQ   r0, #2
      |
        MOVEQ   r0, #0
      ]
        STREQ   r0, vpos
        EXITS   , cxsf
13      ; CR
      [ Margin
        MOV     r0, #5
      |
        MOV     r0, #0
      ]
        STR     r0, hpos
        EXITS   , cxsf
99      ; Delete
        ADR     lr, screenbase
        LDMIA   lr, {r0-r3}
        LDR     lr, [lr, #log2bpc - screenbase]
      [ Margin
        SUB     r2, r2, #1
        CMP     r2, #5
        EXITS   CC, cxsf
      |
        SUBS    r2, r2, #1
        EXITS   MI, cxsf
      ]
        STR     r2, hpos
        MOV     lr, r2, LSL lr
        MUL     r3, r1, r3
        ADD     lr, lr, r3, LSL #4
        ADD     r2, r0, lr ; screenbase + hpos<<log2bpc + 16*linelength*vpos
        MOV     r0, #' '
        MOV     lr, pc
        LDR     pc, PlotChar
        EXITS   , cxsf


font    RN      1
screen  RN      2
bigfont RN      3
mask    RN      4
tophalf RN      5
bottomhalf RN   6
byte    RN      8
scrbyte RN      9
scrbyte2 RN     10
linelen RN      11


PlotChar1
PlotChar2
PlotChar4
        ; To do, if we ever see the like again
        MOV     pc, lr


        ; PlotChar8
        ;
        ; Simple routine to plot a double-height character to the
        ; screen in an 8 bpp screen mode
        ;
        ; On entry:
        ;   R0 = character to output
        ;   R1 = VDU variable LineLength
        ;   R2 -> screen address to plot at
        ;   R4 -> text expand table

PlotChar8
        Entry   "r4-r11"
        MOV     linelen, r1
        ADR     font, HardFont-32*8
        ADD     font, font, r0, LSL #3
        MOV     bigfont, r4
        LDMIA   font, {tophalf, bottomhalf}

        MOV     mask, #&FF000000

        AND     byte, mask, tophalf, LSL #24
        ADD     byte, bigfont, byte, LSR #21
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        STMIA   screen, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen

        AND     byte, mask, tophalf, LSL #16
        ADD     byte, bigfont, byte, LSR #21
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        STMIA   screen, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen

        AND     byte, mask, tophalf, LSL #8
        ADD     byte, bigfont, byte, LSR #21
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        STMIA   screen, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen

        AND     byte, mask, tophalf
        ADD     byte, bigfont, byte, LSR #21
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        STMIA   screen, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen

        AND     byte, mask, bottomhalf, LSL #24
        ADD     byte, bigfont, byte, LSR #21
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        STMIA   screen, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen

        AND     byte, mask, bottomhalf, LSL #16
        ADD     byte, bigfont, byte, LSR #21
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        STMIA   screen, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen

        AND     byte, mask, bottomhalf, LSL #8
        ADD     byte, bigfont, byte, LSR #21
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        STMIA   screen, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen

        AND     byte, mask, bottomhalf
        ADD     byte, bigfont, byte, LSR #21
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        STMIA   screen, {scrbyte, scrbyte2}

        EXIT


        ; PlotChar16
        ;
        ; Simple routine to plot a double-height character to the
        ; screen in a 16 bpp screen mode
        ;
        ; On entry:
        ;   R0 = character to output
        ;   R1 = VDU variable LineLength
        ;   R2 -> screen address to plot at
        ;   R4 -> text expand table

PlotChar16
        Entry   "r4-r11"
        MOV     linelen, r1
        ADR     font, HardFont-32*8
        ADD     font, font, r0, LSL #3
        MOV     bigfont, r4
        LDMIA   font, {tophalf, bottomhalf}

        MOV     mask, #&FF000000
        SUB     linelen, linelen, #16

        AND     byte, mask, tophalf, LSL #24
        ADD     byte, bigfont, byte, LSR #20
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        LDMDB   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen

        AND     byte, mask, tophalf, LSL #16
        ADD     byte, bigfont, byte, LSR #20
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        LDMDB   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen

        AND     byte, mask, tophalf, LSL #8
        ADD     byte, bigfont, byte, LSR #20
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        LDMDB   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen

        AND     byte, mask, tophalf
        ADD     byte, bigfont, byte, LSR #20
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        LDMDB   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen

        AND     byte, mask, bottomhalf, LSL #24
        ADD     byte, bigfont, byte, LSR #20
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        LDMDB   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen

        AND     byte, mask, bottomhalf, LSL #16
        ADD     byte, bigfont, byte, LSR #20
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        LDMDB   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen

        AND     byte, mask, bottomhalf, LSL #8
        ADD     byte, bigfont, byte, LSR #20
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        LDMDB   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen

        AND     byte, mask, bottomhalf
        ADD     byte, bigfont, byte, LSR #20
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        ADD     screen, screen, linelen
        LDMDB   byte, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}
        LDMIA   byte!, {scrbyte, scrbyte2}
        STMIA   screen!, {scrbyte, scrbyte2}

        EXIT


        ; PlotChar32
        ;
        ; Simple routine to plot a double-height character to the
        ; screen in a 32 bpp screen mode
        ;
        ; On entry:
        ;   R0 = character to output
        ;   R1 = VDU variable LineLength
        ;   R2 -> screen address to plot at
        ;   R4 -> text expand table

PlotChar32
        Entry   "r4-r12"
        MOV     linelen, r1
        ADR     font, HardFont-32*8
        ADD     font, font, r0, LSL #3
        MOV     bigfont, r4
        LDMIA   font, {tophalf, bottomhalf}
        Push    "bottomhalf"

        MOV     mask, #&FF000000

        AND     byte, mask, tophalf, LSL #24
        ADD     byte, bigfont, byte, LSR #19
        LDMIA   byte,   {R0,R1, R6,R7, R9,R10, R12,LR}
        STMIA   screen, {R0,R1, R6,R7, R9,R10, R12,LR}
        ADD     screen, screen, linelen
        STMIA   screen, {R0,R1, R6,R7, R9,R10, R12,LR}
        ADD     screen, screen, linelen

        AND     byte, mask, tophalf, LSL #16
        ADD     byte, bigfont, byte, LSR #19
        LDMIA   byte,   {R0,R1, R6,R7, R9,R10, R12,LR}
        STMIA   screen, {R0,R1, R6,R7, R9,R10, R12,LR}
        ADD     screen, screen, linelen
        STMIA   screen, {R0,R1, R6,R7, R9,R10, R12,LR}
        ADD     screen, screen, linelen

        AND     byte, mask, tophalf, LSL #8
        ADD     byte, bigfont, byte, LSR #19
        LDMIA   byte,   {R0,R1, R6,R7, R9,R10, R12,LR}
        STMIA   screen, {R0,R1, R6,R7, R9,R10, R12,LR}
        ADD     screen, screen, linelen
        STMIA   screen, {R0,R1, R6,R7, R9,R10, R12,LR}
        ADD     screen, screen, linelen

        AND     byte, mask, tophalf
        ADD     byte, bigfont, byte, LSR #19
        LDMIA   byte,   {R0,R1, R6,R7, R9,R10, R12,LR}
        STMIA   screen, {R0,R1, R6,R7, R9,R10, R12,LR}
        ADD     screen, screen, linelen
        STMIA   screen, {R0,R1, R6,R7, R9,R10, R12,LR}
        ADD     screen, screen, linelen

        Pull    "bottomhalf"
        AND     byte, mask, bottomhalf, LSL #24
        ADD     byte, bigfont, byte, LSR #19
        LDMIA   byte,   {R0,R1, R5,R7, R9,R10, R12,LR}
        STMIA   screen, {R0,R1, R5,R7, R9,R10, R12,LR}
        ADD     screen, screen, linelen
        STMIA   screen, {R0,R1, R5,R7, R9,R10, R12,LR}
        ADD     screen, screen, linelen

        AND     byte, mask, bottomhalf, LSL #16
        ADD     byte, bigfont, byte, LSR #19
        LDMIA   byte,   {R0,R1, R5,R7, R9,R10, R12,LR}
        STMIA   screen, {R0,R1, R5,R7, R9,R10, R12,LR}
        ADD     screen, screen, linelen
        STMIA   screen, {R0,R1, R5,R7, R9,R10, R12,LR}
        ADD     screen, screen, linelen

        AND     byte, mask, bottomhalf, LSL #8
        ADD     byte, bigfont, byte, LSR #19
        LDMIA   byte,   {R0,R1, R5,R7, R9,R10, R12,LR}
        STMIA   screen, {R0,R1, R5,R7, R9,R10, R12,LR}
        ADD     screen, screen, linelen
        STMIA   screen, {R0,R1, R5,R7, R9,R10, R12,LR}
        ADD     screen, screen, linelen

        AND     byte, mask, bottomhalf
        ADD     byte, bigfont, byte, LSR #19
        LDMIA   byte,   {R0,R1, R5,R7, R9,R10, R12,LR}
        STMIA   screen, {R0,R1, R5,R7, R9,R10, R12,LR}
        ADD     screen, screen, linelen
        STMIA   screen, {R0,R1, R5,R7, R9,R10, R12,LR}

        EXIT


        LTORG

