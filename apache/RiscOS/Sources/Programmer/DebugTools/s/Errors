; s.Errors


badswi_err Entry
	ADR	R0, badswi_errblk
	MOV	R1, #0
	MOV	R2, #0
	ADRL	R4, Mod_Title
	SWI	XMessageTrans_ErrorLookup
	SETV
	EXIT
badswi_errblk
	DCD	0x1E6
	DCB	"BadSWI", 0
	ALIGN


where_param_err ROUT
	ADR	R0, where_param_errblk
	SETV
	Pull	"PC"
where_param_errblk
	DCD	DebugToolsErr + 0
	DCB	"Syntax: *Where [<address>|<register>]", 0
	ALIGN


where_area_err ROUT
	ADR	R0, where_area_errblk
	SETV
	Pull	"PC"
where_area_errblk
	DCD	DebugToolsErr + 1
	DCB	"Unknown area type found in SWI DebugTools_Where", 0
	ALIGN


vector_param_err ROUT
	ADR	R0, vector_param_errblk
	SETV
	Pull	"PC"
vector_param_errblk
	DCD	DebugToolsErr + 2
	DCB	"Syntax: *Vectors [<number> | <name>]", 0
	ALIGN


bad_vec_err ROUT
	ADR	R0, bad_vec_errblk
	SETV
	Pull	"PC"
bad_vec_errblk
	DCD	DebugToolsErr + 3
	DCB	"Vector number out of range", 0
	ALIGN


bad_name_err ROUT
	ADR	R0, bad_name_errblk
	SETV
	MOV	PC, LR
bad_name_errblk
	DCD	DebugToolsErr + 4
	DCB	"Unknown vector name", 0
	ALIGN


irq_param_err ROUT
	ADR	R0, irq_param_errblk
	SETV
	Pull	"R10-R11, PC"
irq_param_errblk
	DCD	DebugToolsErr + 5
	DCB	"Syntax: *IRQInfo [0 | 1]", 0
	ALIGN


