/*
 * Name        : setwevent.c
 * Purpose     : Veneer for method ProgInfo_SetWebEvent
 * Description : Sets the event generated by a click on the web page button
 */


#include "kernel.h"
#include "toolbox.h"
#include "proginfo.h"




/*
 * Name        : proginfo_set_web_event
 * Description : Sets the event generated by a click on the web page button 
 * In          : unsigned int flags
 *               ObjectId proginfo
 *               int event
 * Out         : None
 * Returns     : pointer to error block
 */

extern _kernel_oserror *proginfo_set_web_event ( unsigned int flags,
                                                 ObjectId proginfo,
                                                 int event
                                               )
{
_kernel_swi_regs r;

  r.r[0] = flags;
  r.r[1] = (int) proginfo;
  r.r[2] = ProgInfo_SetWebEvent;
  r.r[3] = (int) event;
  return(_kernel_swi(Toolbox_ObjectMiscOp,&r,&r));
}

