/*-- from CLib --*/

#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include "kernel.h"


/*-- from ToolBoxLib --*/

#include "toolbox.h"


/*******************************************************
 * External functions                                  *
 *******************************************************/

/*
 * calls the SWI Toolbox_GetTemplateName
 */
 
extern _kernel_oserror *toolbox_get_template_name ( unsigned int flags,
			       			    ObjectId id,
					            char *buffer,
					            int  buff_size,
					            int *nbytes
					          )
{
_kernel_swi_regs r;
_kernel_oserror *e;

  r.r[0] = flags;
  r.r[1] = (int) id;
  r.r[2] = (int) buffer;
  r.r[3] = buff_size;
  
  if((e = _kernel_swi(Toolbox_GetTemplateName,&r,&r)) == NULL)
  {
    if(nbytes != NULL) *nbytes = r.r[3];
  }
  
  return(e);
}
 
/*******************************************************
 * End                                                 *
 *******************************************************/


