/*-- from CLib --*/

#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include "kernel.h"


/*-- from ToolBoxLib --*/

#include "toolbox.h"


/*******************************************************
 * External functions                                  *
 *******************************************************/

/*
 * calls the SWI Toolbox_RaiseToolboxEvent
 */

extern _kernel_oserror *toolbox_raise_toolbox_event ( unsigned int flags,
                                                      ObjectId id,
                                                      ComponentId component,
                                                      ToolboxEvent *event
                                                    )
{
_kernel_swi_regs r;

  r.r[0] = flags;
  r.r[1] = (int) id;
  r.r[2] = (int) component;
  r.r[3] = (int) event;
  
  return(_kernel_swi(Toolbox_RaiseToolboxEvent,&r,&r));
}

 

/*******************************************************
 * End                                                 *
 *******************************************************/


