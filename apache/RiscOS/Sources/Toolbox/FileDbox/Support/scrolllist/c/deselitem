#include "swis.h"
#include "kernel.h"
#include "toolbox.h"
#include "gadgets.h"

#include "ScrollList.h"

_kernel_oserror *scrolllist_deselect_item(unsigned int flags,
			ObjectId object_id, ComponentId component_id,
			int index)
{
    return _swix(Toolbox_ObjectMiscOp, _INR(0,4),
    	flags, object_id, ScrollList_DeselectItem, component_id, index);
}
