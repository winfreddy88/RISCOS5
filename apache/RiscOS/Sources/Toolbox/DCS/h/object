/* Title:   object.h
 * Purpose: object list handling for the Generic module
 * Author:  IDJ
 * History: 11-Oct-93: IDJ: created
 *
 */


#ifndef __object_h
#define __object_h

#define QClass (1<<8)

typedef struct object
{
  struct object *next;
  ObjectID window;
  ObjectID id;
  int flags;
} Object;


#endif
