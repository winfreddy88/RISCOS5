#define overflowing_initialise()
/*void overflowing_initialise( void );*/
#define overflowing_malloc(x) malloc(x)
/*void *overflowing_malloc( size_t size );*/
#define overflowing_free(x) free(x)
/*void overflowing_free( void *piece );*/
