;;-----------------------------------------------------------------------------
;; Title:    A1 Wimp System
;; Author:   Neil Raine
;;-----------------------------------------------------------------------------

        AREA    |Asm$$Code|, CODE, READONLY

Module_BaseAddr

        GET     Hdr:ListOpts
        GET     Hdr:Macros
        GET     Hdr:System
        GET     Hdr:Machine.<Machine>
        GET     Hdr:CMOS
        GET     Hdr:PublicWS               ; for SvcTable
        GET     Hdr:ModHand
        GET     Hdr:Proc
        GET     Hdr:EnvNumbers
        GET     Hdr:Services
        GET     Hdr:UpCall
        GET     Hdr:VduExt
        GET     Hdr:FSNumbers
        GET     Hdr:HighFSI
        GET     Hdr:HostFS
        GET     Hdr:NDRDebug
        GET     Hdr:Font
        GET     Hdr:Wimp
        GET     Hdr:Messages
        GET     Hdr:NewErrors
        GET     Hdr:Sprite
        GET     Hdr:Variables
        GET     Hdr:FileTypes
        GET     Hdr:Hourglass
        GET     Hdr:ResourceFS
        GET     Hdr:Portable
        GET     Hdr:MsgTrans
        GET     hdr:ColourTran
        GET     hdr:FPEmulator
        GET     hdr:Territory
        GET     hdr:Switcher
        GET     hdr:PDriver
        GET     hdr:ScrBlank
        GET     hdr:OsWords
        GET     hdr:Countries
        GET     hdr:Internatio
        GET     hdr:VFPSupport
        GET     hdr:OSRSI6

        GET     VersionASM

        GET     hdr.WIMPMacros
        GET     hdr.IconHigh

	GBLL	true
	GBLL	false
true	SETL	{TRUE}
false	SETL	{FALSE}

        GBLS    LoadWimpOptions
      [ Options = ""
LoadWimpOptions SETS "GET Options.s.<System>"
      |
LoadWimpOptions SETS "GET Options.s.$Options"
      ]

	GET	Options.s.!Default
        $LoadWimpOptions
	GET     Hdr:ImageSize.<ImageSize>
	GET	Options.s.!Common
	GET	Options.s.!Debug
        GET     Wimp01.s
        GET     Memory.s
        GET     Wimp02.s
        GET     Watchdog.s
      [ ThreeDPatch
        GET     Tiling3D.s
      |
        GET     Tiling.s
      ]
        GET     Wimp03.s
        GET     Wimp04.s
        GET     Wimp05.s
        GET     Wimp06.s
        GET     Iconbar.s
        GET     Wimp07.s
        GET     ProgErrs.s
        GET     Wimp08.s
        GET     Wimp09.s
        GET     Wimp10.s
        GET     NewSWIs.s
        GET     AutoScroll.s
      [ debug
        GET     Trace.s
      ]

        END
