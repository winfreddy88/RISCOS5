Dir <Obey$Dir>
Remove Resources.BootMerge.!RunImage
Remove Resources.FontMerge.!RunImage
Remove Resources.SysMerge.!RunImage
CDir <Install$Dir>.HardDisc4.!Boot.RO400Hook.Res.Configure.!BootSetup
CDir <Install$Dir>.HardDisc4.!Boot.RO400Hook.Res.Configure.!FontSetup
amu_machine install LDFLAGS=-Debug COMPONENT=BootMerge INSTDIR=<Install$Dir>.HardDisc4.!Boot.RO400Hook.Res.Configure.!BootSetup
amu_machine install LDFLAGS=-Debug COMPONENT=FontMerge INSTDIR=<Install$Dir>.HardDisc4.!Boot.RO400Hook.Res.Configure.!FontSetup
amu_machine install LDFLAGS=-Debug COMPONENT=SysMerge  INSTDIR=<Install$Dir>.HardDisc4.!Boot.RO400Hook.Res.Configure
