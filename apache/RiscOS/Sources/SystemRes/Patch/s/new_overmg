        AREA    |asm$$code|,CODE,READONLY

lk      RN 14

a1      RN 0
a2      RN 1
a3      RN 2
a4      RN 3

v1      RN 4
v2      RN 5
v3      RN 6
v4      RN 7
v5      RN 8
v6      RN 9

ovrmgr1 DCD     overlaymanager
        EXPORT  ovrmgr1

overlaymanager
        DCD     -1       ;not a standard patch
        DCD      5       ;patch 5th word with branch to code below

        MOV     R0,#0
        SWI     &2006e               ; XOS_SynchroniseCodeAreas
        LDMIA   R12,{R0-R8,R14,PC}^

        END
