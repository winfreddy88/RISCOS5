typedef struct Font_Block
{
    int FileType;
    int VersionNumber;
    int FontHeight;
    int xlow, ylow, xupper, yupper;
    int Proportional;
    int FillType;
} Font_Block;


typedef struct Char_Block
{
    int Width;
    int SideBearing;
    int xlowc, ylowc, xupperc, yupperc;
} Char_Block;


os_error *convertadb(const char *infname,
		      const char *outfname,
		      const char *fontname,
		      metricsstr * metrics);
