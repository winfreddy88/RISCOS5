/* udpalette.h */

typedef unsigned int uint32;

typedef struct
{
  uint32 version;
  uint32 pal256;
  uint32 small_ht[5];
  uint32 large_ht[5];
  uint32 caltables;
  uint32 invpal;
  uint32 ucr;
  uint32 reproinfo;
} palhdr_t;

extern int udpalette(FILE *pal, tables_t *tables, int *tables_offset);

extern int readback_params(FILE *pal, calib_t *calib);
