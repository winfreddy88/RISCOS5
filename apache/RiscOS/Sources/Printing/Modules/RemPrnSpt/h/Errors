/*
 * Our error block allocation
 */

#define ERROR 0x00809c00

#define ERROR_LIST1 	ERROR+0
#define ERROR_LIST2	ERROR+1
#define ERROR_LIST3	ERROR+2
#define ERROR_LIST4	ERROR+3
