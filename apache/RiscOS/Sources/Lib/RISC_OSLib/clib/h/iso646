#pragma force_top_level
#pragma include_only_once

/* iso646.h: ISO 'C' (WG14/N843 Aug 98) library header, section 7.9 */
/* Copyright (C) Element 14 Ltd. 1999 */
/* version 1.00 */

#ifndef __iso646_h
#define __iso646_h

#define and    &&
#define and_eq &=
#define bitand &
#define bitor  |
#define compl  ~
#define not    !
#define not_eq !=
#define or     ||
#define or_eq  |=
#define xor    ^
#define xor_eq ^=

#endif

/* end of iso646.h */
