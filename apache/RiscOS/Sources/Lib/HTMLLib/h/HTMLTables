#ifdef TonyTables

HStream * __html_table_row_start_tag(StreamTokens *st, Tag *next_tag, char **html, char *url, int size);
HStream * __html_table_row_end_tag(StreamTokens *st, Tag *next_tag, char **html, char *url);
