#include "elf.h"

/* This is the ELF hashing function, taken from the ARM ELF specification */
unsigned long elf_hash(const unsigned char *name)
{
  unsigned long h, g;

  for (h = 0; *name != 0; ++name)
  {
    h = (h << 4) + *name;
    g = h & 0xf0000000;
    if (g != 0) h ^= g >> 24;
    h &= ~g;
  }
  return h;
}
