#define MSI_SUPPORT 1

#define IO_MIN   0x00000000
#define IO_MAX   0x0000FFFF
#define MEM_MIN  0x00000001
#define MEM_MAX  0x7FFFFFFF
#define ROM_TYPE NonPrefetchable
#define MSI_MIN  0x0000
#define MSI_MAX  0xFFFF

