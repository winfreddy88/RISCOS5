        GET     Hdr:ListOpts
        GET     Hdr:Macros
        GET     Hdr:System
        GET     Hdr:Services
        GET     Hdr:ModHand
        GET     Hdr:Econet
        GET     Hdr:OsBytes
        GET     Hdr:OsWords
        GET     Hdr:IO.IOC-A1

        TTL     NetMonitor

        AREA    |NetMonitor$$Code|, CODE, READONLY, PIC

                ^ 0, R12
Wk_State0       # 1                                     ; Not sure
Wk_State1       # 1                                     ; Not sure
Wk_FIQsAreMine  # 1
Wk_State3       # 1                                     ; Not sure
Wk_State4       # 1                                     ; Not sure
Wk_State5       # 1                                     ; Not sure
Wk_State6       # 1                                     ; Not sure
Wk_State7       # 1                                     ; Not sure
Wk_TempCharDef  # 9
                # 3
Wk_OriginalFont # 9*(256-32-1)                          ; CharDefs of all non control characters
Wk_TotalSize    * :INDEX: @

Module_BaseAddr
        DCD     Mod_Start - Module_BaseAddr             ; Start offset
        DCD     Mod_Init - Module_BaseAddr              ; Initialisation offset
        DCD     0                                       ; Finalisation offset
        DCD     0                                       ; Service call handler offset
        DCD     Mod_HC_Table - Module_BaseAddr          ; Title string offset
        DCD     Mod_HelpStr - Module_BaseAddr           ; Help string offset
        DCD     Mod_HC_Table - Module_BaseAddr          ; Help and command keyword table offset
        DCD     NetMonitorSWI_Base                      ; SWI chunk number
        DCD     Mod_SWIHandler - Module_BaseAddr        ; SWI handler code offset
        DCD     Mod_SWITable - Module_BaseAddr          ; SWI decoding table offset

PrintHexWordNL
        Push    "R0,R14"
        BL      PrintHexWord
        SWI     XOS_NewLine
        LDMIA   R13!,{R0,PC}^

PrintHexByteNL
        Push    "R0,R14"
        BL      PrintHexByte
        SWI     XOS_NewLine
        LDMIA   R13!,{R0,PC}^                           ; Warning: May be unsafe in 32-bit modes

PrintHexWord
        Push    "R0,R14"
        MOV     R0,R0,ROR #16
        BL      PrintHexHalfWord
        MOV     R0,R0,ROR #16
        BL      PrintHexHalfWord
        LDMIA   R13!,{R0,PC}^                           ; Warning: May be unsafe in 32-bit modes

PrintHexHalfWord
        Push    "R0,R14"
        MOV     R0,R0,ROR #8
        BL      PrintHexByte
        MOV     R0,R0,ROR #24
        BL      PrintHexByte
        LDMIA   R13!,{R0,PC}^                           ; Warning: May be unsafe in 32-bit modes

PrintHexByte
        Push    "R0,R14"
        MOV     R0,R0,ROR #4
        BL      PrintHexNybble
        MOV     R0,R0,ROR #28
        BL      PrintHexNybble
        LDMIA   R13!,{R0,PC}^                           ; Warning: May be unsafe in 32-bit modes

PrintHexNybble
        Push    "R0,R14"
        AND     R0,R0,#&0F
        CMP     R0,#&0A
        ADDCC   R0,R0,#'0'
        ADDCS   R0,R0,#'A'-10
        SWI     XOS_WriteC
        LDMIA   R13!,{R0,PC}^                           ; Warning: May be unsafe in 32-bit modes

Mod_HC_Table
        Command NetMonitor, 0, 0
        DCB     0
        ALIGN

NetMonitor_Help
        DCB     "*NetMonitor starts the network monitor running.  ESCape stops it.", 13

NetMonitor_Syntax
        DCB     "Syntax: *NetMonitor", 0
        ALIGN

NetMonitor_Code
        Push    "R14"
        MOV     R0,#ModHandReason_Enter
        ADR     R1,Mod_HC_Table
        SWI     XOS_Module
        Pull    "PC"

Mod_HelpStr
        DCB     "NetMonitor"
        DCB     9
        DCB     "0.00/0131 (13 Dec 1988) assembled  at 20:10:59.", 0
        ALIGN

Mod_Init
        MOV     R6,R14
        Push    "R8"
        TEQP    PC,#1                                   ; Warning: May be unsafe in 32-bit modes
        MOVNV   R0,R0
        MVN     R8,R8
        TEQP    PC,#3                                   ; Warning: May be unsafe in 32-bit modes
        MOVNV   R0,R0
        Pull    "R2"
        TEQ     R2,R8
        BEQ     skip_11
data_3
        SWI     OS_WriteS
        DCB     "This is a 3 micron ARM: NetMonitor will NOT WORK.", 10, 13
        DCB     "Please have your machine upgraded A.S.A.P.", 0
        ALIGN
code_3
        ADR     R0,data_3                               ; Return the warning as an error too!
        ORRS    PC,R6,#V_bit                            ; Warning: May be unsafe in 32-bit modes
skip_11                                                 
        LDR     R3,[R12],#0                             ; Warning: Use the LDR Rd,[Rn,#0] form in preference
        TEQ     R3,#0
        MOVNE   R12,R3
        BNE     skip_27
        MOV     R0,#ModHandReason_Claim
        LDR     R3,workspace_size
        SWI     XOS_Module
        MOVVS   PC,R6
        STR     R2,[R12],#0                             ; Warning: Use the STR Rd,[Rn,#0] form in preference
        MOV     R12,R2
        B       skip_27
skip_27                                                 
        MOV     R0,#0
        STRB    R0,Wk_State0
        STRB    R0,Wk_State1
        STRB    R0,Wk_FIQsAreMine
        MOV     PC,R6

Mod_Start
        SWI     OS_GetEnv
        MOV     R13,R1
        MOV     R0,#OsByte_VDUVariable
        MOV     R1,#8                                   ; TWLCol
        SWI     OS_Byte
        MOV     R3,R1
        MOV     R1,#10                                  ; TWRCol
        SWI     OS_Byte
        SUB     R0,R1,R3
        ADD     R0,R0,#1
        Push    "R0,R1,R14"
        MOV     R1,PC
        SWI     XOS_WriteS
        DCB     "Total width = &", 0
code_5                                                  
        BL      PrintHexByteNL
        TEQP    R1,#0                                   ; Warning: May be unsafe in 32-bit modes
        Pull    "R0,R1,R14"                             ; Debugger warning: Not R8-R14
        MOV     R0,R0,ASR #1
        SUB     R0,R0,#&12
        Push    "R0,R1,R14"
        MOV     R1,PC
        SWI     XOS_WriteS
        DCB     "Used  width = &", 0
code_4                                                  
        BL      PrintHexByteNL
        TEQP    R1,#0                                   ; Warning: May be unsafe in 32-bit modes
        Pull    "R0,R1,R14"                             ; Debugger warning: Not R8-R14
        SWI     NetMonitor_StartWithInternalFont
        SWI     OS_Exit

Mod_SWIHandler
        LDR     R12,[R12],#0                            ; Debugger warning: Rd=Rn
        CMP     R11,#(switable2 - switable1) :SHR: 2
        ADRCSL  R0,error_blk_1E8
        ADDCC   PC,PC,R11,LSL #2
        ORRS    PC,R14,#V_bit                           ; Warning: May be unsafe in 32-bit modes
switable1
        B       PrintChar_Code
        B       DefineTask_Code           
        B       AbandonTask_Code          
        B       ConvertFont_Code          
        B       UseFont_Code              
        B       RestoreFont_Code          
        B       StartWithCurrentFont_Code 
        B       StartWithInternalFont_Code
switable2

PrintChar_Code
        Push    "R0,R14"
        AND     R0,R0,#&FF
        LDRB    R14,Wk_State0
        TEQ     R14,#1
        BEQ     skip_31
        TEQ     R0,#1
        BEQ     skip_30
        CMP     R0,#&7E
        BHI     skip_32
        CMP     R0,#&20
        BCC     skip_32
        LDRB    R14,Wk_State1
        TEQ     R14,#0
        BNE     skip_32
        SWI     XOS_WriteC                              ; Left half
        ADDVC   R0,R0,#&80
        SWIVC   XOS_WriteC                              ; Right half
        Pull    "R0,PC"
skip_30                                                 
        STRB    R0,Wk_State0
        Pull    "R0,PC"
skip_31                                                 
        MOV     R14,#0
        STRB    R14,Wk_State0
        CMP     R0,#(chartable2 - chartable1) :SHR: 2
        ADDCC   PC,PC,R0,LSL #2
        B       skip_32
chartable1
        B       skip_34
        B       skip_32
        B       skip_36
        B       skip_37
        B       skip_39
        B       skip_40
        B       skip_26
        B       skip_25
        B       skip_1
        B       skip_5
        B       skip_4
        B       skip_18
        B       skip_32
        B       skip_4
        B       skip_35
chartable2
skip_32
        MOV     R0,R0,ASR #4
        ADD     R0,R0,#&80
        SWI     XOS_WriteC
        LDRVCB  R0,[R13],#0                             ; Warning: Use the LDR Rd,[Rn,#0] form in preference
        ANDVC   R0,R0,#&0F
        ADDVC   R0,R0,#&90
        SWIVC   XOS_WriteC
        Pull    "R0,PC"
skip_34                                                 
        SWI     XOS_WriteI+9
skip_35                                                 
        Pull    "R0,PC"
skip_36                                                 
        MOV     R0,#'o'                                 ; Overrun
        B       skip_2
skip_37                                                 
        MOV     R0,#'d'                                 ; DCD changed state, no clock?
        B       skip_2
skip_39                                                 
        MOV     R0,#'e'                                 ; CRC error
        B       skip_2
skip_40                                                 
        MOV     R0,#'b'                                 ; Aborted handshake
        B       skip_2
skip_26                                                 
        MOV     R0,#'i'                                 ; Idle
        B       skip_2
skip_25                                                 
        MOV     R0,#'v'                                 ; Valid checksum
        B       skip_2
skip_1                                                  
        MOV     R0,#'t'
skip_2                                                  
        SWI     XOS_WriteS
string_Invert                                           
        DCB     23,17,5,1,"Invert",0
        ALIGN
code_2                                                  
        SWIVC   XOS_WriteC                              ; Left half
        ADDVC   R0,R0,#&80
        SWIVC   XOS_WriteC                              ; Right half
        ADR     R0,string_Invert
        SWIVC   XOS_Write0
        Pull    "R0,PC"
skip_4                                                  
        SWI     XOS_NewLine
        Pull    "R0,PC"
skip_5                                                  
        MOV     R0,#0
skip_18                                                 
        STRB    R0,Wk_State1
        Pull    "R0,PC"

DefineTask_Code
        Push    "R0-R2,R14"
        MOV     R1,#Service_ClaimFIQ
        SWI     XOS_ServiceCall
        BVS     code_1
        STRB    PC,Wk_FIQsAreMine                       ; Debugger warning: Byte load/store of PC
        ADR     R0,subroutine_3
        MOV     R1,#&1C                                 ; Was wrongly encoded as DCI &E3AF101C
subroutine                                              
        SUB     R0,R0,R1
        SUB     R0,R0,#8
        MOV     R0,R0,ASR #2
        ADD     R0,R0,#&EA000000
        STR     R0,[R1],#0                              ; Warning: Use the STR Rd,[Rn,#0] form in preference
        LDR     R0,[R13,#0]
        TEQP    PC,#1                                   ; Warning: May be unsafe in 32-bit modes
        MOVNV   R0,R0
        LDR     R8,controller_base
        MOV     R9,#IOC                                 ; Was wrongly encoded as DCI &E3AF9632
subroutine_1                                            
        MOV     R10,R0
        MOV     R13,#&C0
        STRB    R13,[R8,#0]
        LDRB    R13,[R9,#56]
        ORR     R13,R13,#4
        STRB    R13,[R9,#56]
        TEQP    PC,#3                                   ; Warning: May be unsafe in 32-bit modes
        MOVNV   R0,R0
        Push    "R8"
        LDR     R8,controller_base
        MOV     R0,#&C1
        STRB    R0,[R8,#0]
        MOV     R0,#&1E
        STRB    R0,[R8,#12]
        MOV     R0,#0
        STRB    R0,[R8,#4]
        MOV     R0,#&82
        STRB    R0,[R8,#0]
        MOV     R0,#&65
        STRB    R0,[R8,#4]
        Pull    "R8"
code_1                                                  
        STRVS   R0,[R13],#0                             ; Warning: Use the STR Rd,[Rn,#0] form in preference
        Pull    "R0-R2,PC"

; ******************************************************************************
;
; Subroutine (e.g. called with BL)
;
subroutine_3                                            
        MOV     R11,R14
        LDR     R12,[R10,#20]
        LDRB    R13,[R8,#4]
        TST     R13,#&80
        BNE     skip_9
        MOV     R9,#2
        TST     R13,#&40
        BNE     skip_15
        ADD     R9,R9,#1
        TST     R13,#&20
        BNE     skip_15
        ADD     R9,R9,#1
        TST     R13,#&10
        BNE     skip_15
        ADD     R9,R9,#1
        TST     R13,#8
        BNE     skip_15
        ADD     R9,R9,#1
        TST     R13,#4
        BNE     skip_15
        ADD     R9,R9,#1
        TST     R13,#2
        BNE     skip_15
        MOV     R13,#1
        BL      subroutine_8
        MOV     R13,#0
        BL      subroutine_8
        LDR     R9,[R10,#4]
skip_9                                                  
        LDRB    R13,[R8,#8]
        SUBS    R9,R9,#1
        BLE     skip_12
        BL      subroutine_5
loop_1                                                  
        STR     R12,[R10,#20]
        MOV     R13,#&65
        STRB    R13,[R8,#4]
        SUBS    PC,R11,#4                               ; Warning: May be unsafe in 32-bit modes
skip_12                                                 
        BMI     loop_1
        MOV     R13,#1
        BL      subroutine_8
        MOV     R13,#8
        BL      subroutine_8
        MOV     R9,#0
        B       loop_1
skip_15                                                 
        MOV     R13,#1
        BL      subroutine_8
        MOV     R13,R9
        BL      subroutine_8
        B       loop_1

; ******************************************************************************
;
; Subroutine (e.g. called with BL)
;
subroutine_5                                            
        TEQ     R13,#1
        BNE     subroutine_8
        STRB    R13,[R12],#1
        LDR     R13,[R10,#12]
        TEQ     R13,R12
        LDREQ   R12,[R10,#8]
        MOV     R13,#1

; ******************************************************************************
;
; Subroutine (e.g. called with BL)
;
subroutine_8                                            
        STRB    R13,[R12],#1
        LDR     R13,[R10,#12]
        TEQ     R13,R12
        LDREQ   R12,[R10,#8]
        MOV     PC,R14

AbandonTask_Code
        Push    "R0-R2,R14"
        LDRB    R0,Wk_FIQsAreMine
        TEQ     R0,#0
        BEQ     code_1
        Push    "R8,R9"
        LDR     R8,controller_base
        MOV     R0,#&C0
        STRB    R0,[R8,#0]
        MOV     R9,#IOC                                 ; Was wrongly encoded as DCI &E3AF9632
subroutine_9                                            
        LDRB    R0,[R9,#56]
        BIC     R0,R0,#4
        STRB    R0,[R9,#56]
        Pull    "R8,R9"
        MOV     R1,#Service_ReleaseFIQ
        SWI     XOS_ServiceCall
        BVS     code_1
        MOV     R0,#0
        STRB    R0,Wk_FIQsAreMine
        B       code_1

StartWithInternalFont_Code
        Push    "R7,R14"
        MOV     R5,R0
        MOV     R0,#ModHandReason_Claim
        MOV     R3,#&18
        SWI     XOS_Module
        STR     R5,[R2,#4]
        MOV     R5,R2
        MOVVC   R0,#ModHandReason_Claim
        MOV     R3,#&8000
        SWIVC   XOS_Module
        SWIVC   XNetMonitor_UseFont
        B       skip_28

StartWithCurrentFont_Code
        Push    "R7,R14"
        MOV     R5,R0
        MOV     R0,#ModHandReason_Claim
        MOV     R3,#&18
        SWI     XOS_Module
        STR     R5,[R2,#4]
        MOV     R5,R2
        MOVVC   R0,#ModHandReason_Claim
        MOV     R3,#&8000
        SWIVC   XOS_Module
        SWIVC   XNetMonitor_ConvertFont
skip_28                                                 
        Pull    "R7,PC", VS
        STR     R2,[R5,#8]
        STR     R2,[R5,#16]
        STR     R2,[R5,#20]
        ADD     R4,R2,#&1000
        STR     R4,[R5,#12]
        MOV     R7,#9
        STRB    R7,Wk_State7
        MOV     R3,#0
        MOV     R0,R5
        SWI     XNetMonitor_DefineTask
        BVS     skip_16
        MOV     R0,#&0B
        BL      subroutine_12
        BVS     skip_13
loop_3                                                  
        TEQP    PC,#3                                   ; Warning: May be unsafe in 32-bit modes
        BL      subroutine_14
        BVS     skip_13
        BEQ     skip_10                                 ; Warning: Conditional following BL or SWI may not be 32-bit safe
        LDR     R1,[R5,#20]
        LDR     R2,[R5,#16]
        TEQ     R1,R2
        BEQ     loop_3
loop_4                                                  
        BL      subroutine_14
        BVS     skip_13
        BEQ     skip_10                                 ; Warning: Conditional following BL or SWI may not be 32-bit safe
        LDRB    R0,[R2],#1
        CMP     R2,R4
        LDRGE   R2,[R5,#8]
        TEQ     R0,#1
        BEQ     skip_22
        SWI     XNetMonitor_PrintChar
        BVS     skip_13
loop_5                                                  
        AND     R7,R3,#&FF
        TEQ     R7,#&0A
        BNE     skip_33
        ADD     R3,R3,#&0100
        MOV     R0,R3,ASR #8
        CMP     R0,#4
        BLT     code
        LDRB    R0,Wk_State7
        BL      subroutine_12
        BVS     skip_13
        MOV     R3,#&0B
        B       code
skip_33                                                 
        TEQ     R7,#7
        BEQ     skip
        TEQ     R7,#6
        MOVEQ   R3,#7
        STREQB  R0,Wk_State5
        TEQ     R7,#5
        MOVEQ   R3,#6
        TEQ     R7,#4
        MOVEQ   R3,#5
        TEQ     R7,#3
        MOVEQ   R3,#4
        STREQB  R0,Wk_State4
        TEQ     R7,#2
        MOVEQ   R3,#3
        STREQB  R0,Wk_State3
code                                                    
        TEQ     R1,R2
        BNE     loop_4
        STR     R2,[R5,#16]
        B       loop_3

; ******************************************************************************
;
; Subroutine (e.g. called with BL)
;
subroutine_14                                           
        Push    "R1,R2,R14"
        MOV     R0,#OsByte_INKEY
        MOV     R1,#0
        MOV     R2,#0
        SWI     XOS_Byte
        Pull    "R1,R2,PC", VS
        TEQ     R2,#0
        BEQ     skip_41
        TEQ     R2,#&1B                                 ; Escape?
        Pull    "R1,R2,PC"
skip_41                                                 
        CMP     R1,#&61                                 ; ="a"
        RSBGES  R2,R1,#&7A                              ; ="z"
        SUBGE   R1,R1,#&20                              ; =" "
        TEQ     R1,#&54                                 ; ="T"
        LDREQB  R0,Wk_State7
        EOREQ   R0,R0,#9
        STREQB  R0,Wk_State7
        TEQ     PC,#0
        Pull    "R1,R2,PC"
skip                                                    
        MOV     R3,#8
        STRB    R0,Wk_State6
        LDRB    R0,Wk_State3
        LDRB    R7,Wk_State4
        AND     R0,R0,R7
        TEQ     R0,#&FF
        BNE     code
        MOV     R3,#&0C
        LDRB    R0,Wk_State7
        BL      subroutine_12
        BVS     skip_13
        B       code
skip_22                                                 
        STR     R2,[R5,#16]
loop                                                    
        BL      subroutine_14
        BVS     skip_13
        BEQ     skip_10                                 ; Warning: Conditional following BL or SWI may not be 32-bit safe
        LDR     R1,[R5,#20]
        LDR     R2,[R5,#16]
        TEQ     R1,R2
        BEQ     loop
        LDRB    R0,[R2],#1
        CMP     R2,R4
        LDRGE   R2,[R5,#8]
        STR     R2,[R5,#16]
        TEQ     R0,#6
        BEQ     skip_6
        TEQ     R0,#7
        BEQ     loop_3
        TEQ     R0,#0
        BEQ     skip_7
        BL      subroutine_12
        BVS     skip_13
        TEQ     R0,#1
        BEQ     loop_5
        B       loop_3
skip_6                                                  
        TEQ     R3,#0
        MOVEQ   R3,#1
        BEQ     loop_3
        MOV     R0,#6
        BL      subroutine_12
        MOVVC   R0,#&0B
        BLVC    subroutine_12
        BVS     skip_13
        TEQ     R3,#1
        MOVNE   R3,#0
        B       loop_3
skip_7                                                  
        TEQ     R3,#0
        TEQNE   R3,#1
        BEQ     skip_8
        TEQ     R3,#&0B
        TEQNE   R3,#&0A
        MOVEQ   R3,#&0C
        TEQ     R3,#9
        MOVEQ   R3,#&0A
        TEQ     R3,#8
        MOVEQ   R3,#9
        BL      subroutine_12
        BVS     skip_13
        TEQ     R3,#&0C
        BNE     loop_3
        MOV     R0,#&0B
        BL      subroutine_12
        BVS     skip_13
        B       loop_3
skip_8                                                  
        MOV     R0,#&0A
        BL      subroutine_12
        BVS     skip_13
        MOV     R3,#2
        B       loop_3
skip_10                                                 
        MOV     R0,#OsByte_AcknowledgeEscape
        SWI     XOS_Byte
skip_13                                                 
        SWI     XNetMonitor_AbandonTask
skip_16                                                 
        SWI     XNetMonitor_RestoreFont
        SWI     XOS_NewLine
        Pull    "R7,PC"

; ******************************************************************************
;
; Subroutine (e.g. called with BL)
;
subroutine_12                                           
        Push    "R0,R14"
        MOV     R0,#1
        SWI     XNetMonitor_PrintChar
        Pull    "R14"
        MOVVC   R0,R14
        SWIVC   XNetMonitor_PrintChar
        Pull    "PC"

ConvertFont_Code
        Push    "R0-R4,R14"
        BL      subroutine_2
        ADRVCL  R0,internal_font_hex
        BLVC    subroutine_13
        BVS     skip_38
        ADR     R1,Wk_TempCharDef
        ADR     R3,data_6
        MOV     R4,#&20
loop_8                                                  
        STRB    R4,[R1],#0                              ; Warning: Use the STR Rd,[Rn,#0] form in preference
        MOV     R0,#OsWord_ReadCharacterDefinition
        SWI     XOS_Word
        SWIVC   XOS_WriteI+23
        MOVVC   R0,R4
        SWIVC   XOS_WriteC
        BVS     skip_38
        MOV     R2,#1
loop_6                                                  
        LDRB    R0,[R1,R2]
        MOV     R0,R0,ASR #4
        LDRB    R0,[R3,R0]
        SWI     XOS_WriteC
        BVS     skip_38
        ADD     R2,R2,#1
        TEQ     R2,#9
        BNE     loop_6
        SWIVC   XOS_WriteI+23                           ; Redefine character
        ADDVC   R0,R4,#&80
        SWIVC   XOS_WriteC
        BVS     skip_38
        MOV     R2,#1
loop_7                                                  
        LDRB    R0,[R1,R2]
        AND     R0,R0,#&0F
        LDRB    R0,[R3,R0]
        SWI     XOS_WriteC
        BVS     skip_38
        ADD     R2,R2,#1
        TEQ     R2,#9
        BNE     loop_7
        ADD     R4,R4,#1
        TEQ     R4,#&7F
        BNE     loop_8
        CMP     PC,#0
skip_38                                                 
        STRVS   R0,[R13],#0                             ; Warning: Use the STR Rd,[Rn,#0] form in preference
        Pull    "R0-R4,PC"
data_6                                                  
        DCD     &0F0C0300
        DCD     &3F3C3330
        DCD     &CFCCC3C0
        DCD     &FFFCF3F0

UseFont_Code
        Push    "R0,R14"
        BL      subroutine_2
        ADRVCL  R0,internal_font
        BLVC    subroutine_13
        STRVS   R0,[R13],#0                             ; Warning: Use the STR Rd,[Rn,#0] form in preference
        Pull    "R0,PC"

subroutine_2                                            
        Push    "R0-R2,R14"
        MOV     R0,#OsWord_ReadCharacterDefinition
        ADR     R1,Wk_OriginalFont
        MOV     R2,#&20
loop_2                                                  
        STRB    R2,[R1],#0                              ; Warning: Use the STR Rd,[Rn,#0] form in preference
        SWI     XOS_Word
        BVS     skip_14
        ADD     R1,R1,#9
        ADD     R2,R2,#1
        TEQ     R2,#127
        ADDEQ   R2,R2,#1
        TEQ     R2,#256
        BNE     loop_2
        STRB    R2,[R1],#0                              ; Warning: Use the STR Rd,[Rn,#0] form in preference
        CMP     PC,#0
skip_14                                                 
        STRVS   R0,[R13],#0                             ; Warning: Use the STR Rd,[Rn,#0] form in preference
        Pull    "R0-R2,PC"

RestoreFont_Code
        Push    "R0,R14"
        ADR     R0,Wk_OriginalFont
        BL      subroutine_13
        Pull    "R0,PC"

; ******************************************************************************
;
; Subroutine (e.g. called with BL)
;
subroutine_13                                           
        Push    "R1,R2,R14"
        MOV     R1,#9
loop_9                                                  
        LDRB    R2,[R0],#0                              ; Warning: Use the LDR Rd,[Rn,#0] form in preference
        TEQ     R2,#0
        Pull    "R1,R2,PC", EQ
        SWI     XOS_WriteI+23
        SWIVC   XOS_WriteN
        Pull    "R1,R2,PC", VS
        ADD     R0,R0,#9
        B       loop_9

Mod_SWITable
        DCB     "NetMonitor", 0
        DCB     "PrintChar", 0
        DCB     "DefineTask", 0
        DCB     "AbandonTask", 0
        DCB     "ConvertFont", 0
        DCB     "UseFont", 0
        DCB     "RestoreFont", 0
        DCD     0
workspace_size
        DCD     Wk_TotalSize
controller_base
        DCD     EconetController

        ; This is a 16x8 font made by using pairs of 8x8 characters next to eachother,
        ; with an offset of 128.
        ; For example, code 33 with code 161 next to it displays a '!'.
