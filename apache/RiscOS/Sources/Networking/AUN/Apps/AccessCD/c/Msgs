#include "Includes.h"

void error(char *s)
{
    _kernel_oserror e;

    e.errnum = 0;
    strcpy_ctrl(e.errmess, s);
    _swi(MessageTrans_ErrorLookup, _IN(0)|_IN(1)|_IN(2), &e, m_descr, 0);
}

char *msg_lookup(char *s)
{
    return (char *)_swi(MessageTrans_Lookup, _IN(0)|_IN(1)|_IN(2)|_IN(3)|_RETURN(2),
                        m_descr, s, 0);
}
