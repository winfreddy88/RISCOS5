# Project:   AccessCD


# Toolflags:
CCflags = -c -depend !Depend -IC: -throwback 
C++flags = -c -depend !Depend -IC: -throwback
Linkflags = -aif -c++ -o $@ 
ObjAsmflags = -throwback -NoCache -depend !Depend
CMHGflags = 
LibFileflags = -c -o $@
Squeezeflags = $@ 


# Final targets:
@.!RunImage:   @.o.Cache @.o.main @.o.Msgs @.o.pins @.o.Strings @.o.Wimp \
        C:stubs.o 
        link $(linkflags) @.o.Cache @.o.main @.o.Msgs @.o.pins @.o.Strings \
        @.o.Wimp C:stubs.o 


# User-editable dependencies:


# Static dependencies:
@.o.Cache:   @.c.Cache
        cc $(ccflags) -o @.o.Cache @.c.Cache 
@.o.main:   @.c.main
        cc $(ccflags) -o @.o.main @.c.main 
@.o.Msgs:   @.c.Msgs
        cc $(ccflags) -o @.o.Msgs @.c.Msgs 
@.o.pins:   @.c.pins
        cc $(ccflags) -o @.o.pins @.c.pins 
@.o.Strings:   @.c.Strings
        cc $(ccflags) -o @.o.Strings @.c.Strings 
@.o.Wimp:   @.c.Wimp
        cc $(ccflags) -o @.o.Wimp @.c.Wimp 


# Dynamic dependencies:
o.Cache:	c.Cache
o.Cache:	h.Includes
o.Cache:	C:h.kernel
o.Cache:	C:h.swis
o.Cache:	h.ATypes
o.Cache:	h.AccessCD
o.Cache:	h.Variables
o.Msgs:	c.Msgs
o.Msgs:	h.Includes
o.Msgs:	C:h.kernel
o.Msgs:	C:h.swis
o.Msgs:	h.ATypes
o.Msgs:	h.AccessCD
o.Msgs:	h.Variables
o.pins:	c.pins
o.pins:	h.Includes
o.pins:	C:h.kernel
o.pins:	C:h.swis
o.pins:	h.ATypes
o.pins:	h.AccessCD
o.pins:	h.Variables
o.Strings:	c.Strings
o.Strings:	h.Includes
o.Strings:	C:h.kernel
o.Strings:	C:h.swis
o.Strings:	h.ATypes
o.Strings:	h.AccessCD
o.Strings:	h.Variables
o.Wimp:	c.Wimp
o.Wimp:	h.Includes
o.Wimp:	C:h.kernel
o.Wimp:	C:h.swis
o.Wimp:	h.ATypes
o.Wimp:	h.AccessCD
o.Wimp:	h.Variables
o.main:	c.main
o.main:	h.Includes
o.main:	C:h.kernel
o.main:	C:h.swis
o.main:	h.ATypes
o.main:	h.AccessCD
o.main:	h.Variables
o.main:	h.Main
