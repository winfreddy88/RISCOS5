Set Routines$Dir     <Obey$Dir>
Set Routines$Path    <Routines$Dir>.Routines.
Set Routines$Linker  <Routines$Dir>.Resources.Linker
Set RoutinesRes$Path <Routines$Dir>.Resources.UK.,<Routines$Dir>.Resources.
Run RoutinesRes:LocaleRes Tmp$Locale
If "<Tmp$Locale>" <> "UK" Then Set RoutinesRes$Path <Routines$Dir>.Resources.<Tmp$Locale>.,<RoutinesRes$Path>
Unset Tmp$Locale
Set RoutinesMods$Path RoutinesRes:RM.
IconSprites <Routines$Dir>.!Sprites
