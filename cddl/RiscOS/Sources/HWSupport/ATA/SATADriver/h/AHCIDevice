#ifndef INTERFACE_AHCIDEVICE_H
#define INTERFACE_AHCIDEVICE_H

#include <stdint.h>
#include "Global/HALDevice.h"

/** AHCI HAL device structure */
typedef struct sdhcidevice
{
  struct device     dev;
  uint32_t          flags;
}
ahcidevice_t;

#endif
/* In the exported copy of this file, the Hdr2H translation of hdr.AHCIDevice will follow. */
