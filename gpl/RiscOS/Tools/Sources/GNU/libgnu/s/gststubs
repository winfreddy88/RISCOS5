; Note that this exports __gccmain, and as such is not suitable for use with
; libgcc. However, it can still be used with gcc-generated code.


	IMPORT	|Image$$RO$$Base|
	IMPORT	|Image$$RW$$Limit|
	IMPORT	|C$$code$$Base|
	IMPORT	|C$$code$$Limit|
	IMPORT	main, WEAK
	IMPORT	__root_stack_chunk, WEAK

R0	RN	0
R1	RN	1
R2	RN	2
R3	RN	3
R4	RN	4
R5	RN	5
R6	RN	6
R7	RN	7
R8	RN	8
R9	RN	9
R10	RN	10
R11	RN	11
R12	RN	12
R13	RN	13
R14	RN	14
PC	RN	15

	AREA	|Stub$$Code|, CODE, READONLY

	ENTRY
zeroloop
clibinit
|__main|
	EXPORT	|__main|
	SWI	&10
	MOV	R2,R1
	ADR	R0,CLib_stubs
	LDR	R1,=|Image$$RW$$Limit|
	MVN	R3,#0
	MOV	R4,#0
	MVN	R5,#0
	BL	get_stack_chunk_size
	SWI	&A0681
	SWIVS	&2B
	MOV	R6,R6,LSL #16
	CMP	R6,#&60000
	MOVGE	R4,R0
	ADRGE	R0,CLib_kernel_init_block
	MOVGE	R3,#0
	BGE	_kernel_init		; |GSTSTUBS$$data$$1|
error	ADR	R0,error_block
	SWI	&2B
error_block
	&	&800E90
	=	"Shared C Library not loaded or out of date",0
	ALIGN
CLib_stubs
	&	1
	&	|GSTSTUBS$$data$$1|
	&	TrapHandler
	&	|GSTSTUBS$$data$$2|
	&	__errno
	&	2
	&	TrapHandler
	&	|GSTSTUBS$$data$$1$$end|
	&	__errno
	&	|GSTSTUBS$$data$$2$$end|
	&	-1
CLib_kernel_init_block
	&	|Image$$RO$$Base|
	&	CLib_rts_block
	&	Clib_rts_block_end
CLib_rts_block
	&	Clib_rts_block_end-CLib_rts_block
	&	|C$$code$$Base|
	&	|C$$code$$Limit|
	&	CLib_c_str
	&	CLib_c_init
	&	TrapHandler
	&	UncaughtTrapHandler
	&	EventHandler
	&	UnhandledEventHandler
Clib_rts_block_end
get_stack_chunk_size			; hack to keep offsets right for
	LDR	R6,=__root_stack_chunk	; Zap's gststubs recognition code
	TEQ	R6,#0
	MOVEQ	R6,#&1000
	LDRNE	R6,[R6]
	MOV	R6,R6,LSR #10
	MOV	R6,R6,LSL #16
__gccmain
	MOV	PC,R14
CLib_c_str
	=	"C",0,0,0
CLib_c_init
	MOV	R0,#1
	LDR	R1,=|GSTSTUBS$$data$$2|
	STRB	R0,[R1,#277]
	MOV	R0,R13
	STMFD	R13!,{R14}
	ADR	R1,CLib_rts_block
	LDMIB	R1,{R1,R2}
	BL	_clib_initialise	; |GSTSTUBS$$zidata|+4*&44
	LDR	R0,=main
	TEQ	R0,#0
	LDMEQFD	R13!,{PC}^
	MOV	R12,R13
	STMDB	R13!,{R11,R12,R14,PC}
	SUB	R11,R12,#4
	BL	_kernel_command_string	; |GSTSTUBS$$zidata|+4*&07
	LDR	R1,=main
	BL	_main			; |GSTSTUBS$$zidata|+4*&42
	LDMDB	R11,{R11,R13,PC}^

	EXPORT	__gccmain	; this will cause problems with libgcc


	AREA	|GSTSTUBS$$data$$1|, DATA, NOINIT

