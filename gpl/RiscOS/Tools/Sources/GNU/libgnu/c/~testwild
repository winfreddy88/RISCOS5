#include <stdio.h>
#include "ro_wild.h"

char *program_name;


int main(int argc, char *argv[])
{
  int p = 1;
  program_name = argv[0];
  while (argv[p]) {
    char *c;
    ro_wild *wild = ro_wild_init(argv[p]);
    do {
      c = ro_wild_next(wild);
      if (c) puts(c);
    } while (c);
    ro_wild_finish(wild);
    ++p;
  }
  return 0;
}
